{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://arbiter.dev/schemas/v1/parsed-output.schema.json",
  "title": "Arbiter Parsed Output Record",
  "type": "object",
  "additionalProperties": false,
  "required": ["trial_id", "parse_status"],
  "properties": {
    "trial_id": { "type": "integer", "minimum": 0 },
    "parse_status": { "type": "string", "enum": ["success", "failed", "fallback"] },
    "extraction_method": {
      "type": "string",
      "enum": ["fenced", "unfenced", "raw"]
    },
    "embed_text_source": {
      "type": "string",
      "enum": ["decision", "rationale", "raw_content"]
    },
    "confidence": {
      "oneOf": [
        { "type": "string", "enum": ["low", "medium", "high"] },
        { "type": "number", "minimum": 0, "maximum": 1 },
        { "type": "null" }
      ]
    },
    "outcome": { "type": "string" },
    "rationale": { "type": "string" },
    "rationale_truncated": { "type": "boolean" },
    "trace_summary": { "type": "string" },
    "raw_assistant_text": { "type": "string" },
    "embed_text": { "type": "string" },
    "parser_version": { "type": "string" },
    "parse_error": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "message": { "type": "string" },
        "code": { "type": "string" }
      }
    }
  }
}
